{
  "expo": {
    "name": "DER Kasseler",
    "slug": "der-kasseler",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon1.png",
    "splash": {
      "image": "./assets/herkules.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "fortuna.world",
      "supportsTablet": false
    },
    "android": {
      "package": "fortuna.world",
      "permissions": [
        "CAMERA_ROLL",
        "LOCATION"
      ]
    },
    "description": "mi primera APP con React-native"
  }
}
